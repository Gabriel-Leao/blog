<nav aria-label="..." id="pagination">
  <ul class="pagination">
    <% if(result.page == 1) { %>
      <li class="page-item disabled">
        <a class="page-link" href="/articles/page/<%= result.page -1%>" aria-label="Previous">
          <span aria-hidden="true">&laquo;</span>
        </a>
      </li>
      <% } else if(result.page == 2) { %>
        <li class="page-item">
          <a class="page-link" href="/" aria-label="Previous">
            <span aria-hidden="true">&laquo;</span>
          </a>
        </li>
      <% } else { %>
        <li class="page-item">
          <a class="page-link" href="/articles/page/<%= result.page -1%>" aria-label="Previous">
            <span aria-hidden="true">&laquo;</span>
          </a>
        </li>
      <% } %>

      <% if (result.page == 1) { %>
        <li class="page-item active" aria-current="page"><a class="page-link" href="/">1</a></li>
      <% } else { %>
        <li class="page-item" aria-current="page"><a class="page-link" href="/">1</a></li>
      <% } %>

      <% for (let i = 2; i <= result.maxPages; i++) { %>
        <% if (i == result.page) { %>
          <li class="page-item active" aria-current="page"><a class="page-link" href="/articles/page/<%= i %>"><%= i %></a></li>
        <% } else { %>
          <li class="page-item"><a class="page-link" href="/articles/page/<%= i %>"><%= i %></a></li>
        <% } %>
      <% } %>

    <% if ( result.next == false ) { %>
      <li class="page-item disabled">
        <a class="page-link" href="/articles/page/<%= result.page +1%>" aria-label="Next">
          <span aria-hidden="true">&raquo;</span>
        </a>
      </li>
    <% } else { %>
      <li class="page-item">
        <a class="page-link" href="/articles/page/<%= result.page +1%>" aria-label="Next">
          <span aria-hidden="true">&raquo;</span>
        </a>
      </li>
    <% } %>
  </ul>
</nav>