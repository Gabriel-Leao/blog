<%- include('../../partials/header') %>
  <%- include('../../partials/homeNavbar') %>
  <div class="container">
    <% result.articles.rows.map(article => { %>
      <div class="card my-4">
        <div class="card-header">
          <h2><%= article.title %></h2>
        </div>
        <div class="card-body">
          <a href='/articles/<%= article.slug %>' class="btn btn-success">Ler artigo</a>
        </div>
      </div>
    <% }) %>
    <%- include('../../partials/pagination') %>
  </div>
<%- include('../../partials/footer') %>